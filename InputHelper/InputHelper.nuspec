<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>InputHelper</id>
		<version>1.0.20</version>
		<title>InputHelper</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/InputHelper</projectUrl>
		<licenseUrl>https://github.com/dmanning23/InputHelper/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Helper classes for wrapping up input from different devices.</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.5"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.5"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
			</group>

			<!--Win Universal-->
			<group targetFramework="uap10.0">
				<dependency id="MonoGame.Framework.WindowsUniversal" version="3.5"/>
				<dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
			</group>

			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.DesktopGL" version="3.5"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="InputHelper.Android\bin\Release\InputHelper.Android.pdb" target="lib\MonoAndroid10\InputHelper.Android.pdb" />
		<file src="InputHelper.Android\bin\Release\InputHelper.Android.dll" target="lib\MonoAndroid10\InputHelper.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="InputHelper.iOS\bin\Release\InputHelper.iOS.pdb" target="lib\Xamarin.iOS10\InputHelper.iOS.pdb" />
		<file src="InputHelper.iOS\bin\Release\InputHelper.iOS.dll" target="lib\Xamarin.iOS10\InputHelper.iOS.dll" />

		<!--Windows Universal-->
		<file src="InputHelper.WindowsUniversal\bin\Release\InputHelper.WindowsUniversal.pdb" target="lib\uap10.0\InputHelper.WindowsUniversal.pdb" />
		<file src="InputHelper.WindowsUniversal\bin\Release\InputHelper.WindowsUniversal.dll" target="lib\uap10.0\InputHelper.WindowsUniversal.dll" />

		<!--DesktopGL-->
		<file src="InputHelper.DesktopGL\bin\Release\InputHelper.DesktopGL.pdb" target="lib\net451\InputHelper.DesktopGL.pdb" />
		<file src="InputHelper.DesktopGL\bin\Release\InputHelper.DesktopGL.dll" target="lib\net451\InputHelper.DesktopGL.dll" />

	</files>
</package>
